<script>
    function makeAdder(x){
        return function(y){
        return x+y;
        };
    };
    const add = makeAdder(5);
    console.log(add(3));
    // thats work perfactly;
</script>
<script>
    const person ={
        firstName : "Tauqeer",
        lastName : "khan",
        getfullName: function(){
            return `${this.firstName}${this.lastName}`;
        },        
    };
    const myname = person.getfullName();
    console.log(myname);
</script>
