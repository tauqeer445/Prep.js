 <script>
        let a = 10;
        function changeA(){
            a = 5;
        }
        changeA();
        console.log(a);
    </script>
<!--output is 5-->

   <script>
    let x = 5;
    let y = x*2;
    x = 50;
    console.log(y);
   </script>
   <!-- output is 10 -->

<script>
    function calculate(a,b,c){
        return a*b*c;
    }
    
    console.log(calculate(2,3));

</script>
<!-- Output is NaN -->
